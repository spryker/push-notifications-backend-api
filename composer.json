{
    "name": "spryker/push-notifications-backend-api",
    "type": "library",
    "description": "PushNotificationsBackendApi module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/glossary-storage": "^1.8.0",
        "spryker/glue-application": "^1.39.0",
        "spryker/glue-json-api-convention-extension": "^1.0.0",
        "spryker/kernel": "^3.66.0",
        "spryker/push-notification": "^0.1.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.31.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
